{
    "url": "http://www.shopclues.com/",
    "urlname": "shopclues",
    "type": "category",
    "categoryType": "hover",
    "cat": [
        "Men",
        "Women",
        "Mobiles",
        "Electronics",
        "Home & Kitchen",
        "Baby & Kids",
        "Sports & More"
    ],
    "xpath": "",
    "pagination": {
        "paginate_parameters": {
            "scroll_time_wait": 15,
            "page_listing": 80,
            "load_more_xpath": "//*[@id=\"moreProduct\"]",
            "products_path_count": "//*[@id=\"product_list\"]/div[2]/span"
        },
        "type": "step_scroll"
    },
    "submit": "",
    "items": {
        "hover": true,
        "approach": "click",
        "xpath": "//*[@id=\"product_list\"]/div/div/a[2]",
        "fields": {
            "listing": {
                "url": {
                    "xpath_type": "href",
                    "xpath": "//a"
                },
                "image": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "product_title": {
                    "xpath_type": "",
                    "xpath": "//h3"
                }
            },
            "product": {
                "breadcrumb": {
                    "xpath_type": "html",
                    "xpath": "//*[@id=\"main_data\"]/div[1]"
                },
                "specifications": {
                    "xpath_type": "html",
                    "xpath": "//*[@id=\"specification\"]/table/tbody"
                },
                "thumbnail": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "delivery_time": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "material": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "shipping_charge": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "product_id": {
                    "xpath_type": "text",
                    "xpath": "//*[@id=\"main_data\"]/div[4]/span[3]"
                },
                "mrp": {
                    "xpath_type": "text",
                    "xpath": "//*[@id=\"sec_discounted_price_\"]"
                },
                "selling_price": {
                    "xpath_type": "text",
                    "xpath": "//*[@id=\"main_data\"]/div[4]/div[2]/span[1]"
                },
                "color": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "description": {
                    "xpath_type": "html",
                    "xpath": "//*[@id=\"product_description\"]/p"
                },
                "sizes": {
                    "xpath_type": "html",
                    "xpath": "//*[@id=\"main_data\"]/div[4]/div[6]/ul"
                },
                "rating": {
                    "xpath_type": "text",
                    "xpath": "//*[@id=\"main_data\"]/div[5]/div/div[1]/span[1]"
                },
                "images": {
                    "xpath_type": "data-zoom-image",
                    "xpath": "//*[@id=\"zoom_picture_gall\"]"
                },
                "review_url": {
                    "xpath_type": "",
                    "xpath": ""
                },
                "discount": {
                    "xpath_type": "text",
                    "xpath": "//*[@id=\"main_data\"]/div[4]/div[2]/span[3]"
                },
                "product_name": {
                    "xpath_type": "text",
                    "xpath": "//*[@id=\"main_data\"]/div[4]/h1"
                }
            }
        }
    }
}